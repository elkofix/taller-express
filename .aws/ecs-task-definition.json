{
    "family": "express-mongo-task",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::108782067557:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "express_app",
        "image": "108782067557.dkr.ecr.us-east-1.amazonaws.com/express-app:latest",
        "memory": 512,
        "cpu": 256,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 3000,
            "hostPort": 3000
          }
        ],
        "environment": [
          {
            "name": "MONGO_URI",
            "value": "mongodb://mongo:27017"
          }
        ]
      },
      {
        "name": "mongo",
        "image": "108782067557.dkr.ecr.us-east-1.amazonaws.com/mongo-db:latest",
        "memory": 512,
        "cpu": 256,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 27017,
            "hostPort": 27017
          }
        ]
      }
    ]
  }
  